<mixin>
  <display-name>Dataset Format</display-name>
  <form>
    <input name="datasetFormat" type="RadioButton">
      <label>Formatet på resultatet etter spørring</label>
      <occurrences minimum="1" maximum="1"/>
      <config>
        <option value="jsonStat">Json-stat</option>
        <option value="tbml">tbml</option>
      </config>
      <default>jsonStat</default>
    </input>
  </form>
</mixin>